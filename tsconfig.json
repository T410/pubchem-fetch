{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 14",
    "compilerOptions": {
        "lib": [
            "es2020"
        ],
        "module": "ES6",
        "target": "es6",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": false,
        "outDir": "dist",
        "moduleResolution": "Node",
        "allowSyntheticDefaultImports": true
    }
}